<mvc:View
    controllerName="project1.controller.app"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:mvc="sap.ui.core.mvc">
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader>
                <Image src="image/logo.svg" class="sapUiSmallMargin" height="30px" press="onHome" />
                <MenuButton enabled="true" text="물품 요청" buttonMode="Split" defaultAction="onRequest" />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <App class="myAppDemoWT" id="app">
            </App>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>